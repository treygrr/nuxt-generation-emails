{
  "name": "nuxt-generation-emails",
  "version": "1.0.2",
  "description": "A Nuxt module for authoring, previewing, and sending transactional email templates with MJML and Handlebars.",
  "author": "nullcarry@icloud.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/treygrr/nuxt-generation-emails.git"
  },
  "bugs": {
    "url": "https://github.com/treygrr/nuxt-generation-emails/issues"
  },
  "homepage": "https://github.com/treygrr/nuxt-generation-emails#readme",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "email",
    "mjml",
    "handlebars",
    "transactional-email",
    "email-templates",
    "sendgrid"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs",
      "default": "./dist/module.mjs"
    }
  },
  "bin": {
    "nuxt-gen-emails": "./dist/cli/index.mjs"
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    "playground"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "cli:add": "cd playground && node ../dist/cli/index.mjs add",
    "cli:setup": "cd playground && node ../dist/cli/index.mjs setup"
  },
  "dependencies": {
    "@nuxt/kit": "^4.3.1",
    "citty": "^0.2.1",
    "consola": "^3.4.2",
    "handlebars": "^4.7.8",
    "mjml": "^4.18.0",
    "mjml-browser": "^4.18.0",
    "pathe": "^2.0.3"
  },
  "peerDependencies": {
    "nuxt": ">=4.0.0",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.15.2",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.3.1",
    "@nuxt/test-utils": "^4.0.0",
    "@types/mjml": "^4.7.4",
    "@types/mjml-browser": "^4.15.0",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^10.0.2",
    "nuxt": "^4.1.3",
    "typescript": "~5.9.3",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.5"
  }
}
